package OOD1;

public class main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] numOfSpotType = {{2, 3}};
		ParkingLot newLot = new ParkingLot(1, numOfSpotType);
		Vehicle v1 = new Car();
		
		System.out.println(newLot.park(v1));
		Vehicle v2 = new Car();
		System.out.println(newLot.park(v2));
		Vehicle v3 = new Car();
		System.out.println(newLot.park(v3));
	}

}
